package com.agro.utils;

public enum Roles {

	
	 ADMIN("Admin",1), FARMER("Farmer",2), BUYER("Buyer",3);
	
	
	private String value;
	
	private int roleId;
	
	public String getValue() {
		return value;
	}
	
	public int getRoleId() {
		return roleId;
	}

	Roles(String value,int roleId){
		this.value  = value;
		this.roleId = roleId;
	}
	

}
